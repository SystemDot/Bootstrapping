<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
	<TargetFrameworks>netcoreapp3.1; netstandard2.0</TargetFrameworks>
    <AssemblyName>SystemDot.Bootstrapping</AssemblyName>
    <RootNamespace>SystemDot.Bootstrapping</RootNamespace>
    <Version>4.0.0</Version>
    <PackageId>SystemDotBootstrapping</PackageId>
    <Authors>Chris Sampson. James Goodenough</Authors>
    <Company>SystemDot</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/SystemDot/Bootstrapping</PackageProjectUrl>
    <Description>Application bootstrapping that works with in conjunction with IOC</Description>
    <PackageTags>Bootstrapping</PackageTags>
    <RepositoryUrl>https://github.com/SystemDot/Bootstrapping</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Ioc\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SystemDotIoc" Version="4.0.0" />
  </ItemGroup>

</Project>
